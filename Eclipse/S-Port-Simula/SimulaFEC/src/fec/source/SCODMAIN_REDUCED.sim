             ( s_indexv  &                   !-1;
               s_endif   & s_endif       & 
               s_pushc   & s_c_oaddr     )); !+1;
%      goto InIsTest;
%  InIsTest:
      implNone:=true; !*** make SURE!!! ***;
      outtag(opq.ftag+prototypedisp);
      
%     IIFSBcl= '!3!',  ! test expression (in if-statement);
