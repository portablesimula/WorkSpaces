%+T			STRC2("SCODMAIN",sourceline,"POP");
%+T		 end;
         outtext( if ReuseProc
           then ( s_precall & RTS_e_pro1     &    !+1;
                  s_call    & RTS_e_pro2     )    !-1;
           else ( s_pushv   & RTS_curins     &         !+1;
