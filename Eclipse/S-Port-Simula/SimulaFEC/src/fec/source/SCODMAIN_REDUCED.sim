    when
    ILE do begin
       if opilo=IIFSBcl then begin
%+T         STRC3("SCODMAIN",sourceline,"FJUMPIF","GT " & edit(curindex));
            outtext( s_fjumpif & s_gt );
            goto relPjmp; end;
       if opilo=IIFcl then begin
%+T         STRC3("SCODMAIN",sourceline,"IF","LE");
            outtext( s_if & s_le );
            asd:=asd-2; pushasd;
            goto NXT end;
%+T    STRC3("SCODMAIN",sourceline,"COMPARE","LE");
       outtext( s_compare & s_le ); asd:=asd-1;
  end;

    when
    IGE do begin
       if opilo=IIFSBcl then begin
%+T         STRC3("SCODMAIN",sourceline,"FJUMPIF","LT " & edit(curindex));
            outtext( s_fjumpif & s_lt );
            goto relPjmp; end;
       if opilo=IIFcl then begin
%+T         STRC3("SCODMAIN",sourceline,"IF","GE");
            outtext( s_if & s_ge );
            asd:=asd-2; pushasd;
            goto NXT end;
%+T    STRC3("SCODMAIN",sourceline,"COMPARE","GE");
