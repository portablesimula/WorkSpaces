% +K                  s_goto    & s_eseg & s_empty ) )
% +K      else

         if ReuseProc then begin
            if opq.type = INOTY then begin
%+T				STRC3("SCODMAIN",sourceline,"PRECALL",ED_TTAG(RTS_e_pro1));
%+T				STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_e_pro2));
