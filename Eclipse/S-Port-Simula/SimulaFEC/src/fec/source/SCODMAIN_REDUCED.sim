%        end;
%+T		 if ReuseProc then begin
%+T			STRC3("SCODMAIN",sourceline,"PRECALL",ED_TTAG(RTS_e_pro1));
%+T			STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_e_pro2));
%+T		 end else begin
%+T			STRC3("SCODMAIN",sourceline,"PUSHV",ED_TTAG(RTS_curins));
%+T			STRC3("SCODMAIN",sourceline,"REMOTEV",ED_TTAG(RTS_inst_dl));
