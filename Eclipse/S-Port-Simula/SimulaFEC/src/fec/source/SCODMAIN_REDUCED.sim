    procedure doCodePOWER; begin

%      optx(base type)           opt(exp type):routine
%           int         int:iipower - real:rrpower - long:ddpower
%           real        int:ripower - real:rrpower - long:rdpower
%           long        int:dipower - real:drpower - long:ddpower

      if opilo='!1!' then begin ! ********************* infix power ;
%+T         if opt<IREAL then
%+T               if optx<IREAL then STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_iipowr1))
%+T          else if optx=IREAL then STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_ripowr1))
%+T          else        !ILONG;     STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_dipowr1))
%+T    else if opt=IREAL then
%+T               if optx<IREAL then begin STRC3("SCODMAIN",sourceline,"CONVERT",ED_TTAG(tag_real));
%+T                                        STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_rrpowr1));
%+T          end else if optx=IREAL then   STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_rrpowr1))
%+T          else        !ILONG;           STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_drpowr1))
%+T    else        !ILONG;
%+T               if optx<IREAL then begin STRC3("SCODMAIN",sourceline,"CONVERT",ED_TTAG(tag_real));
%+T                                        STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_rdpowr1));
%+T          end else if optx=IREAL then   STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_rdpowr1))
%+T          else        !ILONG;           STRC3("SCODMAIN",sourceline,"ASSCALL",ED_TTAG(RTS_ddpowr1))
      	  outtext( ! infix power ;
            if opt<IREAL then (
                  if optx<IREAL then ( s_asscall & RTS_iipowr1 )
             else if optx=IREAL then ( s_asscall & RTS_ripowr1 )
             else        !ILONG;     ( s_asscall & RTS_dipowr1 ) )
       else if opt=IREAL then (
                  if optx<IREAL then ( s_convert & tag_real &
                                       s_asscall & RTS_rrpowr1 )
             else if optx=IREAL then ( s_asscall & RTS_rrpowr1 )
             else        !ILONG;     ( s_asscall & RTS_drpowr1 ) )
       else        !ILONG;
                  if optx<IREAL then ( s_convert & tag_real &
                                       s_asscall & RTS_rdpowr1 )
             else if optx=IREAL then ( s_asscall & RTS_rdpowr1 )
             else        !ILONG;     ( s_asscall & RTS_ddpowr1 )   )

      end else begin ! ********************* postfix power ;
%+T       STRC2("SCODMAIN",sourceline,"ASSPAR");
%+T       if opt<IREAL then begin
%+T               if optx<IREAL then STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_iipowr2))
%+T          else if optx=IREAL then STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_ripowr2))
%+T          else        !ILONG;     STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_dipowr2))
%+T       end else if opt=IREAL then begin
%+T              if optx<=IREAL then STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_rrpowr2))
%+T              else    !ILONG;     STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_drpowr2))
%+T       end else begin      !ILONG;
%+T              if optx<=IREAL then STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_rdpowr2))
%+T              else    !ILONG;     STRC3("SCODMAIN",sourceline,"CALL",ED_TTAG(RTS_ddpowr2));
%+T       end;
          outtext(   ! postfix power;
            if opt<IREAL then (
                  if optx<IREAL then ( s_asspar  &
                                       s_call    & RTS_iipowr2 )
             else if optx=IREAL then ( s_asspar  &
                                       s_call    & RTS_ripowr2 )
             else        !ILONG;     ( s_asspar  &
                                       s_call    & RTS_dipowr2 ) )
       else if opt=IREAL then (
                 if optx<=IREAL then ( s_asspar  &
                                       s_call    & RTS_rrpowr2 )
                 else    !ILONG;     ( s_asspar  &
                                       s_call    & RTS_drpowr2 ) )
        else      !ILONG;
                 if optx<=IREAL then ( s_asspar  &
                                       s_call    & RTS_rdpowr2 )
                 else    !ILONG;     ( s_asspar  &
                                       s_call    & RTS_ddpowr2 ) );
         asd:=asd-1; !*** asspar;
     end;
%+D  optx:=IVIRT;
 end;


    procedure doCodeASS_TO_NAME; begin ! ASS-TO-NAME, form par in opq;
            addrPARQNT;
%+T			STRC3("SCODMAIN",sourceline,"SELECTV",ED_TTAG(RTS_parqnt_sem));          
%+T			STRC3("SCODMAIN",sourceline,"PUSHC","FALSE");          
%+T			STRC3("SCODMAIN",sourceline,"IF","NE");          
            outtext( s_selectv & RTS_parqnt_sem  &
                     s_pushc   & s_false         &        !+1;
                     s_if      & s_ne            );       !-2;
            tmpasd:=
            asd:=asd-1; pushasd;
            addrPARQNT;
